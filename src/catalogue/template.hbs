{{#if viewType}}
<div class="mb-catalogue-line">
  <div class="mb-catalogue-tabs">
    <div class="mb-tab js-tab active">Neu im Store</div>
    <div class="mb-tab js-tab">Meist angesehen</div>
  </div>
</div>  
{{else}}
<div class="mb-catalogue-header row">
  <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
    <h2 class="mb-catalogue-total">Angebote.</h2>
  </div>
  <div class="hidden-xs col-sm-6 col-md-6 col-lg-6 text-right">
    <select class="form-control mb-catalogue-select" name="" id="">
      <option value="">Neueste Angebote</option>
      <option value="">Beliebteste Angebote</option>
    </select>
    <label for="field-1" class="mb-select-arrow"></label>
  </div>
</div>
{{/if}}

<div class="mb-catalogue-items row">
  {{#if viewType}}
  <div class="mb-catalogue-dark-block col-xs-12 col-sm-6 col-md-4 col-lg-4">
    <div class="mb-dark-block-title">Haben Sie noch Fragen zu
      <br/>unseren Angeboten?
    </div>
    <ul class="mb-dark-block-list">
      <li class="js-call-callback-modal"><i class="mb-icon-phone"></i>Rückruf anfordern <i class="pull-right md-icon-right-angle"></i></li>
      <li><i class="mb-icon-chat"></i>Live-Chat starten <span class="pull-right md-icon-right-angle"></span></li>
      <li class="js-support-modal"><i class="mb-icon-people"></i>Beratung vor Ort vereinbaren <span class="pull-right md-icon-right-angle"></span></li>
      <li class="js-call-email-modal"><i class="mb-icon-letter"></i>Anfrage per E-Mail senden <span class="pull-right md-icon-right-angle"></span></li>
    </ul>
  </div>
  {{/if}}
  <div class="mb-catalogue-items-region"></div>
  {{#if viewType}}
  <div class="mb-catalogue-item col-xs-12 col-sm-6 col-md-4 col-lg-4">
    <div class="mb-item-img-wrapper mb-full-height">
      <img src="/images/content/All_Offers_Teaser.png" alt=""/>
    </div>
    <div class="mb-item-title-var-two">Alle Angebote entdecken und Wunschfahrzeug finden.</div>
  </div>
  {{/if}}
  {{#unless viewType}}
    <div class="mb-catalogue-dark-block col-xs-12 col-sm-6 col-md-4 col-lg-4">
      <div class="mb-dark-block-title">Haben Sie noch Fragen zu
        <br/>unseren Angeboten?
      </div>
      <ul class="mb-dark-block-list">
        <li class="js-call-callback-modal"><i class="mb-icon-phone"></i>Rückruf anfordern <i class="pull-right md-icon-right-angle"></i></li>
        <li><i class="mb-icon-chat"></i>Live-Chat starten <span class="pull-right md-icon-right-angle"></span></li>
        <li class="js-support-modal"><i class="mb-icon-people"></i>Beratung vor Ort vereinbaren <span class="pull-right md-icon-right-angle"></span></li>
        <li class="js-call-email-modal"><i class="mb-icon-letter"></i>Anfrage per E-Mail senden <span class="pull-right md-icon-right-angle"></span></li>
      </ul>
    </div>
  {{/unless}}
</div>
<div class="mb-pagination-region"></div>
<div class="mb-catalogue-overlay"></div>
